{
  "name": "rootedtoyou",
  "version": "1.0.0",
  "description": "Backend for rootedtoyou",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon --legacy-watch index.js",
    "stage": "env-cmd -f ./config/stage.env pm2 start index.js",
    "prod": "env-cmd -f ./config/prod.env pm2 start index.js",
    "stop": "pm2 stop index",
    "debug": "nodemon --inspect=0.0.0.0  index.js"
  },
  "keywords": [
    "admin"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "razorpay": "^2.9.6",
    "sequelize": "^6.6.2"
  }
}
